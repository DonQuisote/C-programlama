#include <stdio.h> 
/* genişlik çarpı yükseklik Alanı verir */
int genislik;  // genişlik      
int h;       //  yükseklik

int alan;      //  Alan   
int r;      // çevre

int main() {
	h = 7;
	genislik = 5;

    r = 2*(h + genislik);
	printf("Dikdörtgenin çevresi = %d metre\n", r);
	
	alan = h * genislik;
	printf("Dikdörtgen alanı= %d metre kare\n", alan);

return(0);}
